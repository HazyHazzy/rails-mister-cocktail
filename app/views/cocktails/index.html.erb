<h1>Discover the best Cocktails</h1>

<div>
<% @cocktails.each do |cocktail| %>
  <ul>
    <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
  </ul>
  <% end %>
</div>


<div class="btn btn-primary">
  <%= link_to "Show us your cocktail", new_cocktail_path, class: "btn btn-primary" %>
</div>


